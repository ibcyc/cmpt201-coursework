#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *buff = NULL;
  size_t size = 0;
  while (1) {
    printf("Enter programs to run.\n> ");
    ssize_t num_char = getline(&buff, &size, stdin);
    if (num_char > 0 && buff[num_char - 1] == '\n') {
      buff[num_char - 1] = '\0';
      num_char--;
    }
    if (num_char == 0) {
      continue;
    }
    printf("%s", buff);
  }
  free(buff);
  return 0;
}
